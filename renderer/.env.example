# Copy this file to .env and fill in your Stripe details
# These are used by the Stripe Buy Button in DonationBar.jsx

# Required to render the embedded Stripe Buy Button
VITE_STRIPE_PUBLISHABLE_KEY=pk_live_or_test_XXXXXXXXXXXX
VITE_STRIPE_BUY_BUTTON_ID=buy_btn_XXXXXXXXXXXX

# Optional fallback link. Used if Buy Button is not configured.
VITE_STRIPE_PAYMENT_LINK_URL=https://buy.stripe.com/test_XXXXXXXXXXXX
